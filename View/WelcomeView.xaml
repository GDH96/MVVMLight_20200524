<Window x:Class="MVVMLight_20200524.View.WelcomeView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVMLight_20200524.View"
        mc:Ignorable="d"
        Title="WelcomeView" Height="300" Width="300"
        DataContext="{Binding Source={StaticResource Locator},Path=Welcome}" >
    <Grid>
        <StackPanel>
            <TextBlock Text="{Binding Welcome.Introduction}" FontSize="40" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
